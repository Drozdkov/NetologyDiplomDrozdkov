							<v8:lang>ru</v8:lang>
							<v8:content>Анализ выполненных работ и выставленных актов заказчикам</v8:content>
						</v8:item>
					</dcscor:value>
				</dcscor:item>
			</dcsset:outputParameters>
			<dcsset:item xsi:type="dcsset:StructureItemGroup">
